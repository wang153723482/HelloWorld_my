<mapper namespace="com.ibigsea.bootdao.mapper.UserMapper">

    
    
    <insert id="sa" parameterType="com.ibigsea.bootdao.entity.User">
        insert into t_user(username,age) values(#{userName,jdbcType=VARCHAR},#{age,jdbcType=NUMERIC})
